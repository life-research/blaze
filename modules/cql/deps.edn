{:paths ["src" "resources"]

 :deps
 {blaze/db
  {:local/root "../db"}

  com.fasterxml.jackson.module/jackson-module-jaxb-annotations
  {:mvn/version "2.12.3"}

  info.cqframework/cql-to-elm
  {:mvn/version "1.5.2"
   :exclusions
   [com.google.code.javaparser/javaparser
    info.cqframework/qdm
    junit/junit
    xpp3/xpp3
    xpp3/xpp3_xpath
    org.slf4j/jcl-over-slf4j
    org.slf4j/slf4j-api]}

  javax.xml.bind/jaxb-api
  {:mvn/version "2.4.0-b180830.0359"}

  systems.uom/systems-quantity
  {:mvn/version "2.0.2"}

  systems.uom/systems-ucum
  {:mvn/version "2.0.2"}}

 :aliases
 {:test
  {:extra-paths ["test"]

   :extra-deps
   {blaze/db-stub
    {:local/root "../db-stub"}

    lambdaisland/kaocha
    {:mvn/version "1.0.829"}

    org.clojars.akiel/iota
    {:mvn/version "0.1"}

    org.clojure/data.xml
    {:mvn/version "0.2.0-alpha6"}

    org.clojure/test.check
    {:mvn/version "1.1.0"}}

   :main-opts ["-m" "kaocha.runner"]}

  :test-coverage
  {:extra-paths ["test"]

   :extra-deps
   {blaze/db-stub
    {:local/root "../db-stub"}

    org.clojars.akiel/cloverage
    {:mvn/version "1.2.2-SNAPSHOT"}

    org.clojars.akiel/iota
    {:mvn/version "0.1"}

    org.clojure/data.xml
    {:mvn/version "0.2.0-alpha6"}

    org.clojure/test.check
    {:mvn/version "1.1.0"}}

   :main-opts ["-m" "cloverage.coverage" "--codecov" "-p" "src" "-s" "test"
               "-e" "blaze.elm.spec"]}}}
